{
    "name": "jdecano/events-starter",
    "type": "library",
    "description": "Easily start up your recurring events application.",
    "keywords": [
        "Jdecano",
        "events-starter"
    ],
    "homepage": "https://github.com/johndavedecano/events-starter",
    "license": "MIT",
    "authors": [
        {
            "name": "John Dave Decano",
            "email": "johndavedecano@gmail.com",
            "homepage": "http://johndavedecano.me",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
        "rlanvin/php-rrule": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jdecano\\EventsStarter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jdecano\\EventsStarter\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
